{
  "listen": ":36712",
  "stream_buffer": 33554432,
  "receive_buffer": 33554432,
  "max_connections": 2000,
  "timeout": 300,
  "udp_gateway": "127.0.0.1:7300",
  "authentication": {
    "enabled": true,
    "user_db_file": "/etc/udp/users.db"
  },
  "logging": {
    "enabled": true,
    "level": "debug",
    "file": "/var/log/udp-custom.log"
  },
  "rate_limit": {
    "enabled": true,
    "connections_per_ip": 100,
    "packets_per_second": 1000
  },
  "security": {
    "allow_ip_ranges": [
      "0.0.0.0/0"
    ],
    "block_ip_ranges": []
  }
}